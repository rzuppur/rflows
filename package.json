    "pug": "^3.0.1",
    "pug-plain-loader": "^1.0.0",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "tiptap-commands": "^1.11.0",
    "typescript": "^3.6.2",
    "vue-template-compiler": "^2.6.10"
  },
  "engines": {
    "node": ">= 10.0.0",
    "npm": ">= 6.0.0"
  },
