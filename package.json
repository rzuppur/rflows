    "intersection-observer": "^0.7.0",
    "portal-vue": "^2.1.6",
    "pug-cli": "^1.0.0-alpha6",
    "resize-observer-polyfill": "^1.5.1",
    "sockjs-client": "^1.4.0",
    "tiptap": "^1.25.0",
    "tiptap-extensions": "^1.27.0",
    "ts-optchain": "^0.1.8",
    "vue": "^2.6.10",
    "vue-async-computed": "^3.7.0",
    "vue-focus-lock": "^1.3.2",
    "vue-highlight-words": "^1.2.0",
    "webstomp-client": "^1.2.6"
